{
  "name": "poc",
  "main": "webpack.config.js",
  "scripts": {
    "dev:dist": "webpack --mode development",
    "build": "webpack --mode production",
    "watch": "webpack --watch",
    "test": "react-scripts-ts test --env=jsdom",
    "start": "npm run build && node src/server/index.js",
    "client": "webpack-dev-server --mode development --devtool inline-source-map --hot",
    "server": "nodemon src/server/index.js",
    "dev": "concurrently NODE_ENV=development \"npm run server\" \"npm run client\"",
    "debug": "npm run build && node --inspect src/server/index.js"
  },
  "devDependencies": {
    "@babel/core": "^7.2.2",
    "@babel/preset-env": "^7.3.1",
    "@babel/preset-react": "^7.0.0",
    "@types/enzyme": "^2.8.12",
    "@types/enzyme-adapter-react-16": "^1.0.4",
    "babel-jest": "^24.1.0",
    "babel-loader": "^8.0.5",
    "clean-webpack-plugin": "^1.0.1",
    "concurrently": "^4.1.0",
    "css-loader": "^2.1.0",
    "enzyme": "^3.8.0",
    "enzyme-adapter-react-16": "^1.9.1",
    "enzyme-to-json": "^3.3.5",
    "file-loader": "^3.0.1",
    "handlebars-helper-css": "^0.1.0",
    "html-webpack-plugin": "^3.2.0",
    "jest": "^24.1.0",
    "nodemon": "^1.18.10",
    "react-addons-test-utils": "^15.6.2",
    "react-scripts-ts": "^3.1.0",
    "react-test-renderer": "^16.8.1",
    "redux-devtools-extension": "^2.13.8",
    "style-loader": "^0.23.1",
    "ts-loader": "^5.3.3",
    "typescript": "^3.3.3",
    "url-loader": "^1.1.2",
    "webpack": "^4.29.3",
    "webpack-cli": "^3.2.3",
    "webpack-dev-server": "^3.1.14"
  },
  "dependencies": {
    "@types/jest": "^24.0.4",
    "@types/react": "^16.8.2",
    "@types/react-dom": "^16.8.0",
    "@types/react-redux": "^7.0.1",
    "awesome-typescript-loader": "^5.2.1",
    "axios": "^0.18.0",
    "babel-polyfill": "^6.26.0",
    "bootstrap": "^4.3.1",
    "cheerio": "^1.0.0-rc.2",
    "config": "^3.0.1",
    "cookie-parser": "^1.4.4",
    "express-handlebars": "^3.0.1",
    "express-interceptor": "^1.2.0",
    "express-session": "^1.15.6",
    "handlebars-layouts": "^3.1.4",
    "http-errors": "^1.7.2",
    "lodash": "^4.17.11",
    "mongodb": "^3.1.13",
    "morgan": "^1.9.1",
    "mssql": "^4.3.2",
    "node-sass": "^4.11.0",
    "path": "^0.12.7",
    "react": "^16.8.1",
    "react-bootstrap": "^1.0.0-beta.5",
    "react-dom": "^16.8.1",
    "react-redux": "^6.0.0",
    "redux": "^4.0.1",
    "redux-observable": "^1.0.0",
    "redux-saga": "^1.0.1",
    "redux-thunk": "^2.3.0",
    "rxjs": "^6.4.0"
  },
